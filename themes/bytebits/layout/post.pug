nav.navbar
    div.navbar-brand
        a( src = "/" )
            = "ByteBits"

    ul.navbar-items
        each route, name in theme.menu
            li.navbar-item
                a( href = url_for( route ) )
                    = name

main
    header.article-header
        figure.icon
            i.devicon-javascript-plain
        div.title
            = "Map, filter and reduce"

    section.content.article-body
        != page.content
